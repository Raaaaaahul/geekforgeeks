def maxEven(num):
    num = str(num)
    odd_digits = [digit for digit in num if int(digit) % 2 != 0]
    even_digits = [digit for digit in num if int(digit) % 2 == 0]
    
    if len(odd_digits) == 0:
        return int(num)
    
    max_even_num = ""
    for i in range(len(num)):
        if i % 2 == 0:
            max_even_num += pop(even_digits)
        else:
            max_even_num += pop(odd_digits)
    
    return int(max_even_num)

print(maxEven(121))